{
  "name": "circleci-test",
  "version": "1.0.0",
  "description": "- check the build when only send the build",
  "main": "index.js",
  "scripts": {
    "showEnv": "printenv",
    "test": "jest . --ci --runInBand --reporters=default --reporters=jest-junit --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheekit/circleci-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cheekit/circleci-test/issues"
  },
  "homepage": "https://github.com/cheekit/circleci-test#readme",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*.test.{js,jsx}"
    ],
    "coverageDirectory": "./reports/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/bin/",
      "/sql/",
      "/reports/"
    ]
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0"
  }
}
